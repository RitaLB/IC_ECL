find arcos v1

input: outputs = Truth table outputs ; inputs = Truth table inputs, nbit_output = number of output to be analized 
1. arcs <- all all possible input vector pairs (l,h)

   // Selecting traniotion arcs where output changes
2. foreach (l, h) ∈ arcs do
3.	if outputs[l][nbit_output] = output[h][nbit_output] then
4.		remove (l,h) arc from arcs

  // Selecting transition arcs whrere only one input changes 
5. foreach (l,h) ∈ arcs do
6.	sum <- 0
7.	foreach input ∈ inputs do
8.		sum <- sum + (input[l] ^ input[h])	
9.	if sum != 1 then
10.		remove (l,h) arc from arcs

11. return arcs


